<launch>
  <arg name="sim" default="false" />
  <arg name="limited" default="false"/>
  <arg name="debug" default="false" />
  <arg name="real" default="false"/>


  <remap if="$(arg real)" from="/follow_joint_trajectory" to="/l_arm_controller/follow_joint_trajectory"/>
  <remap if="$(arg real)" from="/follow_joint_trajectory" to="/r_arm_controller/follow_joint_trajectory"/>
  
  <!-- Launch moveit -->
	  <include file="$(find husky_dual_ur5_moveit_config)/launch/move_group.launch">
	    <arg name="limited" default="$(arg limited)" />
	    <arg name="debug" default="$(arg debug)" />
          </include>

</launch>
